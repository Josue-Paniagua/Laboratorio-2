
// declaracion de variables


int num1, num2;
// pdiendo a la consola que me imprima y que pida los datos que quiero del usuario 

Console.WriteLine("Ingrese un numero: ");
num1 = Convert.ToInt32(Console.ReadLine());
Console.WriteLine("Ingrese otro numero: ");
num2 = Convert.ToInt32(Console.ReadLine());
// mi condicion para saber que numero es mayor al otro o si son iguales, en este caso utilizar un if combinado con <> = 

if (num1 < num2)
{
    Console.WriteLine("EL numero " + num1 + " es menor que el numero " + num2);
}
else if (num1 > num2)
{
    Console.WriteLine("EL numero " + num1 + " es mayor que numero  " + num2);
}
else if (num1 == num2)
{ Console.WriteLine("Los dos numeros son iguales ");
}
